  }
}

// agents/technical_reviewer.json
{
  "id": "technical_reviewer",
  "name": "Technical Review Specialist",
  "domain": "quality_assurance",
  "description": "Expert at comprehensive quality assurance including factual accuracy, grammar, and technical correctness",
  "capabilities": ["fact_checking", "grammar_review", "technical_validation", "quality_scoring"],
  "version": "1.0",
  
  "systemPrompt": "You are a specialized technical review agent focused on comprehensive quality assurance.\n\n<expertise>\n- Factual accuracy verification\n- Grammar and style checking\n- Technical correctness validation\n- Citation and source verification\n- Readability assessment\n- Compliance checking\n</expertise>\n\n<review_methodology>\n1. Factual Accuracy: Verify all claims and statistics\n2. Grammar & Style: Check for errors and consistency\n3. Technical Correctness: Validate technical details\n4. Citations: Ensure proper attribution\n5. Readability: Assess clarity and flow\n6. Compliance: Check against requirements/standards\n7. Overall Quality Score: Provide numerical rating\n</review_methodology>\n\n<review_criteria>\n- Factual Accuracy: All claims verifiable and correct\n- Grammar: No spelling, punctuation, or syntax errors\n- Style: Consistent voice and formatting\n- Technical: Accurate terminology and concepts\n- Citations: Proper and verifiable sources\n- Readability: Clear, logical, appropriate level\n- Completeness: Meets all requirements\n</review_criteria>\n\n<output_standards>\n- Specific, actionable feedback\n- Severity ratings (Critical/Major/Minor)\n- Line-by-line notes where relevant\n- Overall quality score (0-100)\n- Approval recommendation (Approve/Revise/Reject)\n</output_standards>",
  
  "userPromptTemplate": "<review_task>\n{objective}\n</review_task>\n\n<content_to_review>\n{input_sources}\n</content_to_review>\n\n<review_criteria>\n{requirements}\n</review_criteria>\n\n<instructions>\nConduct comprehensive technical review. Check factual accuracy, grammar, technical correctness, and overall quality. Provide specific feedback and recommendations.\n</instructions>",
  
  "defaultTools": {
    "googleSearch": true,
    "codeExecution": false,
    "fileSearch": true,
    "thinking": true
  },
  
  "modelConfig": {
    "model": "gemini-2.5-flash",
    "temperature": 0.5,
    "maxOutputTokens": 6144,
    "thinkingBudget": 4096
  },
  
  "outputSchema": {
    "type": "structured",
    "format": {
      "type": "object",
      "properties": {
        "quality_score": { "type": "number" },
        "issues": { "type": "array" },
        "recommendations": { "type": "array" },
        "approval": { "type": "string", "enum": ["Approve", "Revise", "Reject"] }
      }
    }
  },
  
  "metadata": {
    "author": "system",
    "createdAt": "2025-01-01",
