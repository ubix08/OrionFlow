  "modelConfig": {
    "model": "gemini-2.5-flash",
    "temperature": 0.6,
    "maxOutputTokens": 6144,
    "thinkingBudget": 4096
  },
  
  "outputSchema": {
    "type": "structured",
    "format": {
      "type": "object",
      "properties": {
        "optimized_content": { "type": "string" },
        "meta_title": { "type": "string" },
        "meta_description": { "type": "string" },
        "recommendations": { "type": "array" },
        "seo_score": { "type": "number" }
      }
    }
  },
  
  "metadata": {
